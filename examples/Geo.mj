class Point {
  int x;
  int y;
  Point origine;

  Point(int a, int b){
    x = a;
    y = b;
    origine = null;
    }

  int getx() {
    return x;
    }
    
  int gety() {
    return y;
    }
    
  void setx(int l) {
    x = l;
    }
    
  void sety(int l) {
    y = l;
    }
  
  void init (int xi,int yi) {
    x = xi;
    y = yi;
    }
 
  void xtrans(int d){
    x = x + d;
    }

  void ytrans(int d){
    y = y + d;
    }
    
  Point milieu (Point p){
    Point m = new Point();
    m.init((x + p.x)/2, (y + p.y)/2);
    return m;
    }
  }


class PointCol extends Point {
  int col;

  PointCol (int xi,int yi, int ci) {
    x = xi;
    y = yi;
    col = ci;
    }

  void changer(int val){
    col = val;
    }
  
  void init (int xi,int yi) {
    col = 84;
    }

  int type () {
    return 2;
    } 
  
  }

